{
    "name": "stefanorg/zucchi",
    "description": "Zucchi Component Library for Zend Framework 2",
    "type": "library",
    "keywords": [
        "library",
        "zf2"
    ],
    "homepage": "https://github.com/stefanorg/Zucchi",
    "authors" : [{
        "name": "Matt Cockayne",
        "email" : "matt@zucchi.co.uk",
        "homepage" : "http://devincharge.com/"
    }],
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-view": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-debug": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-form": "2.*",
        "zendframework/zend-mail": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-inputfilter": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-uri": "2.*",
        "zendframework/zend-servicemanager": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Zucchi": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
